{
    "name": "josualeonard/inpsyde-users",
    "description": "Load user list and details from API call",
    "minimum-stability": "stable",
	"license": "GPL-2.0-or-later",
	"type": "wordpress-plugin",
	"homepage": "https://github.com/josualeonard/inpsyde-users",
    "scripts": {
		"test": "./vendor/bin/phpunit --testdox",
		"testw": ".\\vendor\\bin\\phpunit --testdox",
        "phpcs": "./vendor/bin/phpcs --standard=vendor/inpsyde/php-coding-standards/Inpsyde/ruleset.xml tests views src InpsydeUsers.php",
        "phpcsw": ".\\vendor\\bin\\phpcs --standard=vendor\\inpsyde\\php-coding-standards\\Inpsyde\\ruleset.xml tests views src InpsydeUsers.php"
	},
    "require": {
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "inpsyde/php-coding-standards": "^0.13.4",
        "squizlabs/php_codesniffer": "^3.6",
        "brain/monkey": "2.*",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "InpsydeUsers\\": "src/InpsydeUsers"
        }
    },
    "authors": [
        {
            "name": "Josua",
            "email": "josua.leonard@gmail.com"
        }
    ]
}
